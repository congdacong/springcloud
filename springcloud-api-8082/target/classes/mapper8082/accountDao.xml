<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.producer8082.dao.AccountDao">

    <select id="findAll" resultType="com.producer8082.entity.Account">
        select * from account
    </select>

    <select id="findById" resultType="com.producer8082.entity.Account" parameterType="String">
        select * from account where id = #{id}
    </select>

    <delete id="delete"  parameterType="String">
        DELETE FROM account where id = #{id}
    </delete>
</mapper>